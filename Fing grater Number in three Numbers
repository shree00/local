if(a>b){

if(a>c)
{
    cout<<"A is grater : "<<a<<endl;
}

else {
    cout<<"C is grater : "<<c<<endl;
}

}


else{
    if(b>c){
    cout<<"B is grater : "<<b<<endl;
}
else{
    cout<<"C is grater : "<<c<<endl;
}

